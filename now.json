{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "app/static/$1"
    },
    { "src": "/favicon.ico", "dest": "app/favicon.ico" },
    {
      "src": "/(.*)",
      "dest": "app/$1",
      "continue": true
    }
  ],
  "build": {
    "env": {
      "SENTRY_DSN": "@sk-sentry-dsn",
      "SANITY_PROJECT_ID": "@sk-sanity-project-id",
      "SANITY_DATASET": "@sk-sanity-dataset",
      "SANITY_AUTH_TOKEN": "@sk-sanity-auth-token",
      "SHOPIFY_SHOP_NAME": "spinellikilcollin",
      "SHOPIFY_STOREFRONT_TOKEN": "@sk-storefront-token",
      "POSTMARK_KEY": "@sk-postmark-key",
      "MAILCHIMP_LIST_ID": "2c5420dc8f",
      "MAILCHIMP_API_KEY": "@sk-mailchimp-api-key",
      "ALGOLIA_API_KEY": "@sk-algolia-api-key",
      "ALGOLIA_APP_ID": "@sk-algolia-app-id"
    }
  }
}
