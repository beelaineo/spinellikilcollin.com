{
	"name": "spinellikilcollin-root",
	"private": true,
	"scripts": {
		"setup": "yarn install",
		"test": "yarn workspace spinellikilcollin-app test",
		"build": "yarn workspace spinellikilcollin-app build",
		"build:analyze": "yarn workspace spinellikilcollin-app build:analyze",
		"start:app": "yarn workspace spinellikilcollin-app start",
		"ncu": "yarn exec ncu && yarn workspaces run ncu",
		"ncu:u": "yarn exec ncu -u && yarn workspaces run ncu -u && yarn install",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.4",
		"@commitlint/config-conventional": "^12.1.4",
		"@types/eslint": "^7.28.0",
		"@types/eslint-plugin-prettier": "^3.1.0",
		"@types/prettier": "^2.3.2",
		"@types/tsc-watch": "^4.2.0",
		"@typescript-eslint/eslint-plugin": "^4.28.3",
		"@typescript-eslint/parser": "^4.28.3",
		"dotenv": "^10.0.0",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.24.0",
		"husky": "^7.0.1",
		"lerna": "^4.0.0",
		"lint-staged": "^11.0.1",
		"npm-check-updates": "^11.8.2",
		"prettier": "^2.3.2",
		"tsc-watch": "^4.4.0",
		"vercel": "^23.0.1",
		"yarnhook": "^0.5.1"
	},
	"workspaces": [
		"app",
		"sanity"
	],
	"lint-staged": {
		"*.(json|js|jsx|ts|tsx|html|css|md)": [
			"prettier --write"
		],
		"*(.js|jsx|ts|tsx)": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"react-icons": "^4.2.0"
	},
	"resolutions": {
		"graphql": "^14.6.0"
	},
	"version": "1.22.15",
	"packageManager": "yarn@1.18.0"
}
