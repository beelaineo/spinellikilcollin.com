	"scripts": {
		"dev": "NODE_ENV=development next dev",
		"start": "DEBUG=app* next",
		"start:prod": "NODE_ENV=production next",
		"start:debug": "NODE_OPTIONS='--inspect' next dev",
		"start:algolia": "DEBUG=api:algolia,dev:sentry yarn next",
		"build": "yarn build:next",
		"build:next": "next build",
		"build:analyze": "ANALYZE=true next build",
		"build:json": "DEBUG=build:json yarn ts-node --project tsconfig.scripts.json src/scripts/getSpreadsheetJSON.ts",
		"install-types": "typesync && yarn install",
		"generate-types": "graphql-codegen -r dotenv/config && prettier ./src/types/**/* --write",
		"lint": "yarn eslint src",
		"lint:watch": "yarn eslint-watch src",
		"test": "jest",
		"test:watch": "jest --watch",
		"webhooks:list": "ts-node --project tsconfig.scripts.json ./scripts/registerWebhooks.ts list",
		"webhooks:clear": "ts-node --project tsconfig.scripts.json ./scripts/registerWebhooks.ts clear",
		"webhooks:register": "ts-node --project tsconfig.scripts.json ./scripts/registerWebhooks.ts register"
	},
