{
  "name": "spinellikilcollin-sanity",
  "version": "0.0.0",
  "description": "Sanity configuration for spinellikilcollin.com",
  "author": "Joseph Thomas <joseph@good-idea.studio>",
  "private": true,
  "license": "ISC",
  "main": "lib/sanity.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "yarn deploy:graphql && yarn deploy:cms",
    "deploy:cms": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --generation gen2 --playground && yarn workspace spinellikilcollin-app generate-types",
    "sync:export": "sanity dataset export production",
    "sync:reset": "sanity dataset delete staging && sanity dataset create staging",
    "sync:import": "sanity dataset import production.tar.gz staging --replace",
    "sync:cleanup": "rm production.tar.gz",
    "sync": "yarn sync:reset && yarn sync:export && yarn sync:import && yarn sync:cleanup"
  },
  "dependencies": {
    "@reach/auto-id": "^0.18.0",
    "@sane-shopify/sanity-plugin": "^0.24.0",
    "@sanity/base": "^2.35.4",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/color-input": "^2.36.0",
    "@sanity/components": "^2.14.0",
    "@sanity/core": "^2.35.3",
    "@sanity/default-layout": "^2.35.4",
    "@sanity/default-login": "^2.35.4",
    "@sanity/desk-tool": "^2.35.4",
    "@sanity/production-preview": "^2.35.3",
    "@sanity/vision": "2.35.4",
    "@types/eslint": "^7.29.0",
    "@types/prettier": "^2.7.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.1",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "rxjs": "^7.8.0",
    "sanity-plugin-media": "^1.5.1",
    "styled-components": "^5.3.6",
    "tsc-watch": "^4.6.2"
  },
  "devDependencies": {
    "@good-idea/unwind-edges": "^1.5.3",
    "@sanity/cli": "^3.2.4",
    "@sanity/client": "^3.4.1",
    "@types/eslint": "^7.29.0",
    "@types/lodash": "^4.14.191",
    "@types/prop-types": "^15.7.5",
    "@types/ramda": "^0.28.21",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "lodash": "^4.17.21",
    "ndjson-parse": "^1.0.4",
    "ramda": "^0.28.0",
    "react-hot-loader": "^4.13.1"
  }
}
