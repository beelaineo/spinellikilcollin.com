{
  "name": "spinellikilcollin-sanity",
  "version": "0.0.0",
  "description": "Sanity configuration for spinellikilcollin.com",
  "author": "Joseph Thomas <joseph@good-idea.studio>",
  "private": true,
  "engines": {
    "node": "^18.x"
  },
  "license": "ISC",
  "main": "lib/sanity.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "sanity start",
    "build": "sanity build",
    "dev": "sanity dev",
    "test": "sanity check",
    "deploy": "yarn deploy:graphql:production && yarn deploy:cms && yarn deploy:types",
    "deploy:cms": "sanity deploy",
    "deploy:graphql:production": "sanity graphql deploy --generation gen2 --playground",
    "deploy:graphql:staging": "sanity graphql deploy --dataset staging --generation gen2 --playground",
    "deploy:types": "yarn workspace spinellikilcollin-app generate-types",
    "sync:export": "sanity dataset export production",
    "sync:reset": "sanity dataset delete staging && sanity dataset create staging",
    "sync:import": "sanity dataset import production.tar.gz staging --replace",
    "sync:cleanup": "rm production.tar.gz",
    "sync": "yarn sync:reset && yarn sync:export && yarn sync:import && yarn sync:cleanup"
  },
  "dependencies": {
    "@reach/auto-id": "^0.18.0",
    "@sane-shopify/sanity-plugin": "^0.24.0",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/color-input": "^3.1.0",
    "@sanity/components": "^2.14.0",
    "@sanity/production-preview": "^2.35.3",
    "@sanity/vision": "latest",
    "@types/eslint": "^8.40.2",
    "@types/prettier": "^2.7.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "^4.10.1",
    "rxjs": "^7.8.1",
    "sanity": "latest",
    "sanity-plugin-media": "^2.2.2",
    "styled-components": "^5.3.6",
    "tsc-watch": "^6.0.4"
  },
  "devDependencies": {
    "@good-idea/unwind-edges": "^1.5.3",
    "@sanity/cli": "^3.14.1",
    "@sanity/client": "^3.4.1",
    "@types/eslint": "^8.40.2",
    "@types/lodash": "^4.14.195",
    "@types/prop-types": "^15.7.5",
    "@types/ramda": "^0.29.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "lodash": "^4.17.21",
    "ndjson-parse": "^1.0.4",
    "ramda": "^0.29.0",
    "react-hot-loader": "^4.13.1"
  }
}
