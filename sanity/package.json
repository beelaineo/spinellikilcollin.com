{
  "name": "spinellikilcollin-sanity",
  "version": "0.0.0",
  "description": "Sanity configuration for spinellikilcollin.com",
  "author": "Joseph Thomas <joseph@good-idea.studio>",
  "private": true,
  "license": "ISC",
  "main": "lib/sanity.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "yarn deploy:graphql && yarn deploy:cms",
    "deploy:cms": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --generation gen2 --playground && yarn workspace spinellikilcollin-app generate-types",
    "sync:export": "sanity dataset export production",
    "sync:reset": "sanity dataset delete staging && sanity dataset create staging",
    "sync:import": "sanity dataset import production.tar.gz staging --replace",
    "sync:cleanup": "rm production.tar.gz",
    "sync": "yarn sync:reset && yarn sync:export && yarn sync:import && yarn sync:cleanup"
  },
  "dependencies": {
    "@reach/auto-id": "^0.17.0",
    "@sane-shopify/sanity-plugin": "^0.23.0",
    "@sanity/base": "^2.33.2",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/color-input": "^2.33.2",
    "@sanity/components": "^2.14.0",
    "@sanity/core": "^2.33.2",
    "@sanity/default-layout": "^2.33.2",
    "@sanity/default-login": "^2.33.2",
    "@sanity/desk-tool": "^2.33.2",
    "@sanity/production-preview": "^2.33.2",
    "@sanity/vision": "^2.33.2",
    "@types/eslint": "^7.29.0",
    "@types/prettier": "^2.7.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "rxjs": "^7.5.6",
    "sanity-plugin-media": "^1.4.11",
    "styled-components": "^5.3.5",
    "tsc-watch": "^4.6.2"
  },
  "devDependencies": {
    "@good-idea/unwind-edges": "^1.5.3",
    "@sanity/client": "^2.23.2",
    "@types/eslint": "^7.29.0",
    "@types/lodash": "^4.14.185",
    "@types/prop-types": "^15.7.5",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "lodash": "^4.17.21",
    "ndjson-parse": "^1.0.4",
    "ramda": "^0.28.0",
    "react-hot-loader": "^4.13.0"
  }
}
