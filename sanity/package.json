{
  "name": "spinellikilcollin-sanity",
  "version": "0.0.0",
  "description": "Sanity configuration for spinellikilcollin.com",
  "author": "Joseph Thomas <joseph@good-idea.studio>",
  "private": true,
  "license": "ISC",
  "main": "lib/sanity.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "yarn deploy:graphql && yarn deploy:cms",
    "deploy:cms": "yarn sanity deploy",
    "deploy:graphql": "yarn sanity graphql deploy --generation gen2 --playground && yarn workspace spinellikilcollin-app generate-types",
    "sync:export": "sanity dataset export production",
    "sync:reset": "sanity dataset delete staging && yarn sanity dataset create staging",
    "sync:import": "sanity dataset import production.tar.gz staging --replace",
    "sync:cleanup": "rm production.tar.gz",
    "sync": "yarn sync:reset && yarn sync:export && yarn sync:import && yarn sync:cleanup"
  },
  "dependencies": {
    "@sane-shopify/sanity-plugin": "^0.17.6",
    "@sanity/base": "^1.150.7",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/cli": "^1.150.7",
    "@sanity/components": "^1.150.7",
    "@sanity/core": "^1.150.7",
    "@sanity/default-layout": "^1.150.7",
    "@sanity/default-login": "^1.150.1",
    "@sanity/desk-tool": "^1.150.7",
    "@sanity/vision": "^1.150.6",
    "prop-types": "^15.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "sanity-plugin-media": "^0.2.13"
  },
  "devDependencies": {
    "@good-idea/unwind-edges": "^1.4.0",
    "@sanity/client": "^1.150.7",
    "@types/eslint": "^7.2.2",
    "@types/lodash": "^4.14.161",
    "@types/prop-types": "^15.7.3",
    "@types/ramda": "^0.27.15",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "lodash": "^4.17.20",
    "ndjson-parse": "^1.0.4",
    "ramda": "^0.27.1",
    "react-hot-loader": "^4.12.21"
  },
  "resolutions": {
    "@sanity/base": "^0.140.12",
    "react-hot-loader": "^4.8.0:"
  }
}
